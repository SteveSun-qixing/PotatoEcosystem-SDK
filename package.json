{
  "name": "@chips/sdk",
  "version": "0.1.0",
  "description": "Chips SDK - Development toolkit for Chips ecosystem",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "bin": {
    "chips": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "vite build --mode analyze",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "benchmark": "tsx benchmarks/index.ts",
    "benchmark:id": "tsx benchmarks/index.ts id",
    "benchmark:cache": "tsx benchmarks/index.ts cache",
    "benchmark:parser": "tsx benchmarks/index.ts parser",
    "benchmark:renderer": "tsx benchmarks/index.ts renderer",
    "benchmark:memory": "node --expose-gc benchmarks/utils/memory.bench.ts",
    "benchmark:report": "tsx benchmarks/index.ts && open benchmarks/reports/latest.html",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "chips",
    "sdk",
    "card",
    "content",
    "ecosystem"
  ],
  "author": "Chips Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "eventemitter3": "^5.0.1",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "happy-dom": "^20.4.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.2.4",
    "rollup-plugin-visualizer": "^5.14.0",
    "terser": "^5.46.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.2.0"
  },
  "optionalDependencies": {
    "@chips/core": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
